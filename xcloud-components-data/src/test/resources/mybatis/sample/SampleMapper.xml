<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wl4g.components.data.sample.dao.SampleDao" >

  <select id="getSample" resultType="com.wl4g.components.data.sample.bean.SampleBean">
	SELECT
		DATE_FORMAT(NOW(), '%Y-%m-%d %H:%m:%s') AS "nowDate"
	FROM
		DUAL
  </select>

</mapper>